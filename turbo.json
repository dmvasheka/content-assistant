{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "dev": {
      "cache": false,
      "env": [
        "OPENAI_API_KEY",
        "SUPABASE_URL",
        "SUPABASE_KEY"
      ]
    }
  }
}

